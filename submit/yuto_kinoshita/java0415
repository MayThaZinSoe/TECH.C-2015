package test;

public class A {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ

		int x;

		for(int i=1; i<=9; i++){
			for(int j=1; j<=9; j++){
				x = i*j;
				System.out.print(i + "*" + j + "=");
				if(x<10){
					System.out.print(" " + x);
				}else{
					System.out.print(x);
				}
				if(j==9){
					System.out.println();
				}else{
					System.out.print(" ,");
				}
			}
		}

	}

}
